FoamFile
{
    version         2.0;
    format          ascii;

    root            "";
    case            "";
    instance        "";
    local           "";

    class           dictionary;
    object          controlDict;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     Xoodles;
startFrom       startTime;
startTime       0.;
stopAt          endTime;
endTime         0.007;
deltaT          1e-3;
writeControl    adjustableRunTime;
writeInterval   0.007;
purgeWrite      0;
writeFormat     binary;
writePrecision  6;
writeCompression uncompressed;
timeFormat      general;
timePrecision   6;
runTimeModifiable no;
maxCo 		0.8;
adjustTimeStep	true;

